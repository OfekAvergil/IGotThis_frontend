{
  "expo": {
    "name": "IGotThis",
    "slug": "IGotThis",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification":{
      "icon":"./assets/icon.png",
      "color": "#DB2763"
   },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {    
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION"
      ],
      "notification":{
        "icon":"./assets/icon.png",
        "color": "#DB2763"
     }
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
